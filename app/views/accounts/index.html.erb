<p id="notice"><%= notice %></p>

<h1>Projects</h1> <br>


<table class="table table-striped table-hover">
  <thead class="thead-light">
    <tr>
      <th>conta</th>
      <th>name</th>
      <th>agencia</th>
      <th>tipo de conta</th>
      <th>saldo</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @accounts.each do |project| %>
      <tr>
        <td><%= project.number %></td>
        <td><%= project.user.name %></td>
        <td><%= project.agency %></td>
        <td><%= project.type_account %></td>
        <td><%= project.balance %></td>
      </tr>
    <% end %>
  </tbody>
</table>
    
<br>